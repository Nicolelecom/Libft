NAME = libft.a

CFLAGS = -Wall -Wextra -Werror

HEADER = libft.h

SRCS =	ft_isalpha.c ft_isprint.c ft_isascii.c ft_tolower.c \
		ft_toupper.c ft_isalnum.c ft_isdigit.c ft_strncmp.c ft_strlen.c \
		ft_atoi.c ft_strchr.c ft_strrchr.c ft_memset.c ft_bzero.c ft_memcpy.c \
		ft_memcmp.c ft_memchr.c ft_memmove.c ft_strlcpy.c ft_strlcat.c \
		ft_putchar_fd.c ft_putstr_fd.c ft_putendl_fd.c ft_putnbr_fd.c \
		ft_strnstr.c ft_calloc.c ft_strdup.c ft_substr.c ft_strjoin.c \
		ft_striteri.c ft_strmapi.c ft_itoa.c ft_strtrim.c ft_split.c

BONUS_SRC = ft_lstnew_bonus.c ft_lstsize_bonus.c ft_lstadd_front_bonus.c \
			ft_lstlast_bonus.c ft_lstadd_back_bonus.c ft_lstdelone_bonus.c \
			ft_lstclear_bonus.c ft_lstiter_bonus.c ft_lstmap_bonus.c

OBJECTS = $(SRCS:.c=.o)

BONUS_OBJ = $(BONUS_SRC:.c=.o)

all: $(NAME)

$(NAME): $(OBJECTS)
	ar rc $(NAME) $(OBJECTS)

%.o: %.c $(HEADER)
	cc $(CFLAGS) -c $< -o $@

bonus: .bonus

.bonus: $(OBJECTS) $(BONUS_OBJ) 
	ar rc $(NAME) $(OBJECTS) $(BONUS_OBJ)
	@touch  .bonus

fclean: clean
	@echo "$$PIJADA"
	@rm -f $(NAME)
	@rm -f .bonus
	
clean:
	@rm -f $(OBJECTS) $(BONUS_OBJ)

re: fclean all

#I el Pony!!!
.PHONY: all fclean clean re bonus

